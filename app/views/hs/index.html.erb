<p id="notice"><%= notice %></p>

<h1>Hs</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Country</th>
      <th>Name</th>
      <th>School</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hs.each do |h| %>
      <tr>
        <td><%= h.city %></td>
        <td><%= h.country %></td>
        <td><%= h.name %></td>
        <td><%= h.school_id %></td>
        <td><%= h.state %></td>
        <td><%= link_to 'Show', h %></td>
        <td><%= link_to 'Edit', edit_h_path(h) %></td>
        <td><%= link_to 'Destroy', h, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New H', new_h_path %>
